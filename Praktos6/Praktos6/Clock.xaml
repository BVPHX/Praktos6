<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Praktos6.Clock"
             Title="Часы">
    <ContentPage.Content>
        <StackLayout>
            <TimePicker x:Name="timePicker" Format="HH mm ss" HorizontalOptions="Center" VerticalOptions="Center"  FontSize="50"></TimePicker>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>